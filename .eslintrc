{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "arrow-body-style": 0,
    "max-len": [2, 100, 2, {"ignoreUrls": true, "ignorePattern": "^//.*|^import.*"}],
    "new-cap": 0,
    "no-console": 0,
    "no-nested-ternary": 0,
    "no-param-reassign": 0,
    "no-shadow": 0,
    "no-trailing-spaces": 0,
    "prefer-template": 0,
    "react/jsx-no-bind": 0,
    "react/no-did-update-set-state": 0,
    "react/prop-types": 0,
    "react/sort-comp": 0,
    "spaced-comment": 0
  }
}
